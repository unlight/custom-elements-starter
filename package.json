{
  "name": "custom-elements-starter",
  "version": "0.0.0-dev",
  "description": "",
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server --env.watch",
    "#build:entry:prod": "npm run build -- --env.entry example-plain --mode production",
    "#build:entry": "npm run build -- --env.entry example-plain",
    "#dev:entry": "npm run dev -- --env.entry example-plain",
    "#predev": "sh Taskfile predev",
    "build": "webpack",
    "build:prod": "env NODE_ENV=production npm run build -- --mode production",
    "serve": "npm run dev",
    "test:r": "node node_modules/karma/bin/karma start --single-run",
    "test:w": "karma start",
    "test": "npm run eslint && npm run tscheck && npm run test:cov && npm run typecov",
    "test:cov": "npm run test:r -- --collectCoverage",
    "typecov": "node node_modules/type-coverage/bin/type-coverage --detail",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint:w": "watchexec -w src \"npm run eslint -- --cache\"",
    "eslint": "node node_modules/eslint/bin/eslint src --ext ts,tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "tslint:fix": "node node_modules/tslint/bin/tslint -p tsconfig.json --fix",
    "lint:w": "run-p tsclint:w eslint:w",
    "lint:fix": "npm run tslint:fix || true && npm run eslint:fix",
    "commit": "node node_modules/cz-customizable/standalone.js",
    "cmlint": "sh Taskfile commit_lint",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "prestorybook": "npm run custom-elements",
    "storybook:docs": "start-storybook --docs -p 6006",
    "build:storybook": "build-storybook",
    "custom-elements": "wca analyze \"src/**/*.{js,ts,tsx}\" --format json --outFile dist/custom-elements.json",
    "test:m": "node node_modules/stryker-cli/bin/stryker-cli run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test",
      "commit-msg": "git-branch-is -r \"^(release|master|develop)\" && commitlint -E HUSKY_GIT_PARAMS || echo \"Ignoring commitlint error...\""
    }
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/web-components": "^5.3.13",
    "@stryker-mutator/babel-transpiler": "^2.5.0",
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/html-reporter": "^2.5.0",
    "@stryker-mutator/jasmine-framework": "^2.5.0",
    "@stryker-mutator/karma-runner": "^2.5.0",
    "@stryker-mutator/typescript": "^2.5.0",
    "@types/jasmine": "^3.5.4",
    "@types/node": "^13.7.1",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/eslint-plugin-tslint": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-postcss": "^1.0.1",
    "cssnano": "^4.1.10",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^16.1.1",
    "eslint-plugin-wc": "^1.2.0",
    "eslint-plugin-wix-editor": "^3.0.0",
    "fast-glob": "^3.2.0",
    "flat": "^5.0.0",
    "functional-element": "0.0.14",
    "git-branch-is": "^3.1.0",
    "h-document-element": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-source-map-support": "^1.4.0",
    "karma-webpack": "^4.0.2",
    "lit-element": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.3",
    "simplytyped": "^3.2.3",
    "source-map-loader": "^0.2.4",
    "stryker-cli": "^1.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-etc": "^1.10.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.4.0",
    "typescript": "~3.7.5",
    "watchexec-bin": "^1.0.0",
    "web-component-analyzer": "^1.0.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
