{
  "name": "lit-element-starter",
  "version": "0.0.0-dev",
  "description": "",
  "main": "index.js",
  "keywords": [
    "lit-element",
    "typescript",
    "rollup"
  ],
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "node node_modules/rollup/dist/bin/rollup --config",
    "build:libs": "npm run build -- --environment LIBS",
    "build:libs:prod": "npm run build -- --environment LIBS,MINIFY,PRODUCTION",
    "build:prod": "npm run build -- --environment MINIFY,PRODUCTION",
    "dev": "npm run build -- --watch",
    "predev": "sh Taskfile predev",
    "serve": "npm run dev",
    "test:r": "node node_modules/karma/bin/karma start --single-run",
    "test:watch": "karma start",
    "buildtest:w": "npm run build -- --environment TEST --watch",
    "test:w": "run-p \"buildtest:w\" \"test:watch\"",
    "test": "npm run eslint && npm run tscheck && npm run test:cov && npm run typecov",
    "test:cov": "npm run test:r -- --collectCoverage",
    "typecov": "node node_modules/type-coverage/bin/type-coverage --detail",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint:w": "watchexec -w src \"npm run eslint -- --cache\"",
    "eslint": "node node_modules/eslint/bin/eslint src --ext ts",
    "eslint:fix": "npm run eslint -- --fix",
    "tslint:fix": "node node_modules/tslint/bin/tslint -p tsconfig.json --fix",
    "lint:w": "run-p tsclint:w eslint:w",
    "lint:fix": "npm run tslint:fix || true && npm run eslint:fix",
    "commit": "node node_modules/cz-customizable/standalone.js",
    "cmlint": "sh Taskfile commit_lint",
    "semantic-release": "semantic-release",
    "_build": "sh Taskfile build",
    "_bundle": "sh Taskfile bundle",
    "_setupfrontend": "npm i && npm i -D ng-packagr @angular/core @angular/compiler-cli @angular/compiler tsickle",
    "_setupwebpack": "npm i && npm i -D webpack @types/webpack @types/webpack-env webpack-cli webpack-dev-server html-webpack-plugin source-map-loader ts-loader html-loader add-asset-html-webpack-plugin terser-webpack-plugin istanbul-instrumenter-loader swc-loader @swc/core",
    "_setupmutation": "npm i && npm i -D stryker-cli @stryker-mutator/core @stryker-mutator/typescript @stryker-mutator/jest-runner @stryker-mutator/html-reporter",
    "storybook": "start-storybook -p 6006",
    "storybook:docs": "start-storybook --docs -p 6006",
    "build:storybook": "build-storybook",
    "analyze": "wca analyze \"src/**/*.{js,ts}\" --format json --outFile dist/custom-elements.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test",
      "commit-msg": "git-branch-is -r \"^(release|master|develop)\" && commitlint -E HUSKY_GIT_PARAMS || echo \"Ignoring commitlint error...\""
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "lit-element": "^2.2.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@open-wc/karma-esm": "^2.13.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/web-components": "^5.3.12",
    "@types/jasmine": "^3.5.3",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-loader": "^8.0.6",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^16.0.0",
    "eslint-plugin-wix-editor": "^3.0.0",
    "git-branch-is": "^3.1.0",
    "husky": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.3",
    "karma-source-map-support": "^1.4.0",
    "lit-element": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-browsersync": "^1.1.0",
    "rollup-plugin-esm-import-to-url": "^2.0.0",
    "rollup-plugin-lit-styles": "0.0.6",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript-paths": "^1.2.1",
    "semantic-release": "^17.0.2",
    "simplytyped": "^3.2.3",
    "tslint": "^6.0.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-etc": "^1.10.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.4.0",
    "typescript": "~3.7.5",
    "watchexec-bin": "^1.0.0",
    "web-component-analyzer": "^1.0.2"
  }
}
